source 'https://github.com/CocoaPods/Specs.git'         # CocoaPods 중앙 레포지토리
platform :ios, '10.0'
use_frameworks!
inhibit_all_warnings!

target 'RealWeather' do
  pod 'FBSDKLoginKit'
  pod 'GoogleSignIn'

  target 'RealWeatherTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RealWeatherUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
